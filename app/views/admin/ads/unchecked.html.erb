<% provide(:title, "Admin | Unchecked posts") %>

<% if @posts.present? %>
  <section class="table-header mb-3">
    <div class="row">
      <div class="col-sm-12 col-md-12">
        <h1 class="text-center heading">Unchecked Posts</h1>
      </div>
    </div>
  </section>

  <%= render "layouts/form" %>
<% else %>
  <h2 class="text-center text-muted mt-5">You have no unchecked post</h2>
<% end %>
    


